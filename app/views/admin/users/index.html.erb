<div class="col-md-8 col-md-offset-2">
  <%= render "shared/import_form", model: User %>
  <h1><%= t "user.all_users" %></h1>  
  <%= link_to t("user.add_user"), new_admin_user_path, class: "btn btn-success btn-lg" %>
  <div class="users"> 
    <%= search_form_for [:admin, @q] do |f| %>  
      <%= f.label :team_name_cont %>
      <%= f.search_field :team_name_cont, class: "form-control", 
                                          placeholder: t("user.placeholder_search") %>
      <%= f.submit class: "btn btn-success" %>
    <% end %>   
    <%= render @users %>
    <%= will_paginate %>
  </div>
</div>