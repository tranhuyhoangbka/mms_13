<tr>
  <td><%= skill.id %></td>
  <td><%= skill.name %></td>
  <td><%= link_to "", edit_admin_skill_path(skill), 
                      class: "glyphicon glyphicon-edit" %></td>
  <td><%= link_to "", [:admin, skill], class: "glyphicon glyphicon-trash",
          method: :delete, data: {confirm: t("general.confirm_delete")} %></td>
                      
</tr>