<% provide :title, @team.name %>
<h1><%= @team.name %></h1>
<h1><%= t "user_list" %></h1>
<div class="col-sm-3 col-md-12">
  <ul class="list-group">
    <% @users.each do |user| %>
      <li class="list-group-item list-group-item-info">
        <b><%= link_to user.name, user_path(user) %></b> | 
        <%= user.email %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate @users %>
</div>